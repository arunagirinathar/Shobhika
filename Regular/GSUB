	sub ardhaNa.dv ardhaDa.dv ardhaDha.dv Ya.dv by NaDaDhaYa.dv;
	sub ardhaNa.dv ardhaDa.dv ardhaDha.dv Va.dv by NaDaDhaVa.dv;
	sub ardhaNa.dv ardhaDa.dv ardhaVa.dv Ya.dv by NaDaVaYa.dv;
	sub ardhaNa.dv ardhaSa.dv ardhaPha.dv Ya.dv by NaSaPhaYa.dv;
	sub ardhaPa.dv ardhaSa.dv ardhaNa.dv Ya.dv by PaSaNaYa.dv;
	sub ardhaMa.dv ardhaPa.dv ardhaSa.dv Ya.dv by MaPaSaYa.dv;
	sub ardhaLa.dv ardhaGa.dv ardhaVa.dv Ya.dv by LaGaVaYa.dv;
	sub ardhaLa.dv ardhaPa.dv ardhaTa.dv Ya.dv by LaPaTaYa.dv;
