	sub ardhaDa.dv ardhaNa.dv by ardhaDaNa.dv;
	sub ardhaDha.dv ardhaNa.dv by ardhaDhaNa.dv;
	sub ardhaPa.dv ardhaNa.dv by ardhaPaNa.dv;
	sub ardhaPha.dv ardhaNa.dv by ardhaPhaNa.dv;
	sub ardhaBa.dv ardhaNa.dv by ardhaBaNa.dv;
	sub ardhaBha.dv ardhaNa.dv by ardhaBhaNa.dv;
	sub ardhaMa.dv ardhaNa.dv by ardhaMaNa.dv;
	sub ardhaYa.dv ardhaNa.dv by ardhaYaNa.dv;
	sub ardhaVa.dv ardhaNa.dv by ardhaVaNa.dv;
	sub ardhaSa.dv ardhaNa.dv by ardhaSaNa.dv;

# Other ardha-forms
	sub ardhaKa.dv ardhaTa.dv by ardhaKaTa.dv;
	sub ardhaTa.dv ardhaTa.dv by ardhaTaTa.dv;
	sub ardhaSa.dv ardhaTaRa.dv by ardhaSaTaRa.dv;
} pres2;

