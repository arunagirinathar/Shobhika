# The next two lookup operations are to get the right output for Ra.dv + matraRu.dv etc. 
lookup abvs_RaMatraRu{
	sub Ra.dv matraRu.dv by RephVowelRu.dv;
	sub Ra.dv matraRuu.dv by RephVowelRuu.dv;
	sub Ra.dv matraLru.dv by RephVowelLru.dv;
	sub Ra.dv matraLruu.dv by RephVowelLruu.dv;
}abvs_RaMatraRu;

lookup abvs_RephVowelRu{
	sub RephVowelRu.dv by vowelRu.dv Reph.dv ;
	sub RephVowelRuu.dv by vowelRuu.dv Reph.dv ;
	sub RephVowelLru.dv by vowelLru.dv Reph.dv ;
	sub RephVowelLruu.dv by vowelLruu.dv Reph.dv ;
}abvs_RephVowelRu;

lookup abvs_Reph{
	sub @matraAboveBase Reph.dv' by Reph_alt.dv;
	sub @MadhyaDanda Reph.dv' by Reph_alt.dv;
}abvs_Reph;

