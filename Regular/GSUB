feature pres { # Pre-base Substitutions

lookup pres4 { # Four-letter conjuncts
	sub ardhaNga.dv ardhaKaSsa.dv ardhaNna.dv Va.dv by NgaKaSsaNnaVa.dv; 
	sub ardhaNga.dv ardhaKa.dv ardhaTaRa.dv Ya.dv by NgaKaTaRaYa.dv;
	sub ardhaTta.dv ardhaSha.dv ardhaVa.dv Ya.dv by TtaShaVaYa.dv;
	sub ardhaKa.dv ardhaTa.dv ardhaVa.dv Ya.dv by KaTaVaYa.dv;
	sub ardhaKa.dv ardhaSa.dv ardhaPa.dv La.dv by KaSaPaLa.dv;
	sub ardhaGa.dv ardhaDa.dv ardhaDha.dv Va.dv by GaDaDhaVa.dv;
	sub ardhaGa.dv ardhaDa.dv ardhaVa.dv Ya.dv by GaDaVaYa.dv;
	sub ardhaNga.dv ardhaKa.dv ardhaTa.dv Ya.dv by NgaKaTaYa.dv;
	sub ardhaNga.dv ardhaKa.dv ardhaTa.dv Va.dv by NgaKaTaVa.dv;
	sub ardhaNga.dv ardhaTa.dv ardhaTa.dv Va.dv by NgaTaTaVa.dv;
	sub ardhaNga.dv ardhaGa.dv ardhaDha.dv Ya.dv by NgaGaDhaYa.dv;
	sub ardhaNga.dv ardhaGa.dv ardhaDha.dv Va.dv by NgaGaDhaVa.dv;
	sub ardhaTa.dv ardhaTa.dv ardhaSa.dv Ya.dv by TaTaSaYa.dv;
	sub ardhaTa.dv ardhaSa.dv ardhaNa.dv Ya.dv by TaSaNaYa.dv;
	sub ardhaTa.dv ardhaSa.dv ardhaMa.dv Ya.dv by TaSaMaYa.dv;
	sub ardhaTa.dv ardhaSa.dv ardhaTha.dv Ya.dv by TaSaThaYa.dv;
	sub ardhaTa.dv ardhaSa.dv ardhaPha.dv Ya.dv by TaSaPhaYa.dv;
	sub ardhaDa.dv ardhaDa.dv ardhaVa.dv Ya.dv by DaDaVaYa.dv;
	sub ardhaDa.dv ardhaDha.dv ardhaNa.dv Ya.dv by DaDhaNaYa.dv;
	sub ardhaNa.dv ardhaTa.dv ardhaTa.dv Va.dv by NaTaTaVa.dv;
	sub ardhaNa.dv ardhaTa.dv ardhaVa.dv Ya.dv by NaTaVaYa.dv;
	sub ardhaNa.dv ardhaTa.dv ardhaSa.dv Ta.dv by NaTaSaTa.dv;
	sub ardhaNa.dv ardhaTa.dv ardhaSa.dv Tha.dv by NaTaSaTha.dv;
	sub ardhaNa.dv ardhaTa.dv ardhaSa.dv Na.dv by NaTaSaNa.dv;
