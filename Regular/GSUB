		sub Bha.dv Vattu.dv by BhaRa.dv;
		sub Ma.dv Vattu.dv by MaRa.dv;
		sub Va.dv Vattu.dv by VaRa.dv;
		sub Sha.dv Vattu.dv by ShaRa.dv;
		sub Sa.dv Vattu.dv by SaRa.dv;
		sub Ha.dv Vattu.dv by HaRa.dv;
		sub Ya.dv Vattu.dv by YaRa.dv;
		sub La.dv Vattu.dv by LaRa.dv;
		sub Ssa.dv Vattu.dv by SsaRa.dv;
} vatu1;

	lookup vatu_half { #Vattu with ardha forms gives ardha-rakar forms
		sub ardhaKa.dv Vattu.dv by ardhaKaRa.dv;
		sub ardhaKha.dv Vattu.dv by ardhaKhaRa.dv;
		sub ardhaGa.dv Vattu.dv by ardhaGaRa.dv;
		sub ardhaGha.dv Vattu.dv by ardhaGhaRa.dv;
		sub ardhaCa.dv Vattu.dv by ardhaCaRa.dv;
		sub ardhaCha.dv Vattu.dv by ardhaChaRa.dv;
		sub ardhaJa.dv Vattu.dv by ardhaJaRa.dv;
		sub ardhaJha.dv Vattu.dv by ardhaJhaRa.dv;
		sub ardhaTta.dv Vattu.dv by ardhaTtaRa.dv;
		sub ardhaTtha.dv Vattu.dv by ardhaTthaRa.dv;
		sub ardhaDda.dv Vattu.dv by ardhaDdaRa.dv;
		sub ardhaDdha.dv Vattu.dv by ardhaDdhaRa.dv;
		sub ardhaTa.dv Vattu.dv by ardhaTaRa.dv;
		sub ardhaTha.dv Vattu.dv by ardhaThaRa.dv;
		sub ardhaDa.dv Vattu.dv by ardhaDaRa.dv;
		sub ardhaDha.dv Vattu.dv by ardhaDhaRa.dv;
		sub ardhaNa.dv Vattu.dv by ardhaNaRa.dv;
		sub ardhaPa.dv Vattu.dv by ardhaPaRa.dv;
		sub ardhaPha.dv Vattu.dv by ardhaPhaRa.dv;
		sub ardhaBa.dv Vattu.dv by ardhaBaRa.dv;
		sub ardhaBha.dv Vattu.dv by ardhaBhaRa.dv;
		sub ardhaMa.dv Vattu.dv by ardhaMaRa.dv;
		sub ardhaVa.dv Vattu.dv by ardhaVaRa.dv;
		sub ardhaSa.dv Vattu.dv by ardhaSaRa.dv;
		sub ardhaSha.dv Vattu.dv by ardhaShaRa.dv;
} vatu_half;

	script deva;
	language HIN;
	lookup vatu2;
	lookup vatu1;
	lookup vatu_half;
}vatu;

