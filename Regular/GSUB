feature vatu { # Vattu
	script deva;
	language dflt;
	lookup vatu2 { # Vattu with Akhand characters
		sub KaSsa.dv Vattu.dv by KaSsaRa.dv;
		sub JaNya.dv Vattu.dv by JaNyaRa.dv;
} vatu2;

	lookup vatu1 { # Vattu with with all other (non-Akhand glyphs)
		sub Ka.dv Vattu.dv by KaRa.dv;
		sub Kha.dv Vattu.dv by KhaRa.dv;
		sub Ga.dv Vattu.dv by GaRa.dv;
