	sub ardhaLa.dv ardhaVa.dv ardhaDha.dv Va.dv by LaVaDhaVa.dv;
	sub ardhaSsa.dv ardhaTta.dv ardhaNa.dv Ya.dv by SsaTtaNaYa.dv;
	sub ardhaSa.dv ardhaTa.dv ardhaTa.dv Ya.dv by SaTaTaYa.dv;
	sub ardhaSa.dv ardhaTa.dv ardhaVa.dv Ya.dv by SaTaVaYa.dv;
	sub ardhaSa.dv ardhaTha.dv ardhaNa.dv Ya.dv by SaThaNaYa.dv;
} pres4;

