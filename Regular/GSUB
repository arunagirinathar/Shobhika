feature rkrf { # Rakar forms
	script dev2;
	language dflt;

	lookup rkrf2 {
		sub KaSsa.dv Halant.dv Ra.dv by KaSsaRa.dv;
		sub JaNya.dv Halant.dv Ra.dv by JaNyaRa.dv;
} rkrf2;

	lookup rkrf1 {
		sub Ka.dv Halant.dv Ra.dv by KaRa.dv;
		sub Kha.dv Halant.dv Ra.dv by KhaRa.dv;
		sub Ga.dv Halant.dv Ra.dv by GaRa.dv;
		sub Gha.dv Halant.dv Ra.dv by GhaRa.dv;
		sub Ca.dv Halant.dv Ra.dv by CaRa.dv;
		sub Ja.dv Halant.dv Ra.dv by JaRa.dv;
		sub Jha.dv Halant.dv Ra.dv by JhaRa.dv;
		sub Nya.dv Halant.dv Ra.dv by NyaRa.dv;
		sub Nna.dv Halant.dv Ra.dv by NnaRa.dv;
		sub Ta.dv Halant.dv Ra.dv by TaRa.dv;
		sub Tha.dv Halant.dv Ra.dv by ThaRa.dv;
		sub Da.dv Halant.dv Ra.dv by DaRa.dv;
		sub Dha.dv Halant.dv Ra.dv by DhaRa.dv;
		sub Na.dv Halant.dv Ra.dv by NaRa.dv;
		sub Pa.dv Halant.dv Ra.dv by PaRa.dv;
		sub Pha.dv Halant.dv Ra.dv by PhaRa.dv;
		sub Ba.dv Halant.dv Ra.dv by BaRa.dv;
		sub Bha.dv Halant.dv Ra.dv by BhaRa.dv;
		sub Ma.dv Halant.dv Ra.dv by MaRa.dv;
		sub Ya.dv Halant.dv Ra.dv by YaRa.dv;
		sub Va.dv Halant.dv Ra.dv by VaRa.dv;
		sub Sha.dv Halant.dv Ra.dv by ShaRa.dv;
		sub Sa.dv Halant.dv Ra.dv by SaRa.dv;
		sub Ssa.dv Halant.dv Ra.dv by SsaRa.dv;
		sub Ha.dv Halant.dv Ra.dv by HaRa.dv;
		sub GaNukta.dv Halant.dv Ra.dv by GaNuktaRa.dv;
		sub JaNukta.dv Halant.dv Ra.dv by JaNuktaRa.dv;
		sub PhaNukta.dv Halant.dv Ra.dv by PhaNuktaRa.dv;
		sub KhaNukta.dv Halant.dv Ra.dv by KhaNuktaRa.dv;
} rkrf1;

	script dev2;
	language HIN;
	lookup rkrf2;
	lookup rkrf1;
} rkrf ;

