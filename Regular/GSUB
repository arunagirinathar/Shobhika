feature akhn { # Akhanda
	sub Ka.dv Halant.dv Ssa.dv by KaSsa.dv;
	sub Ja.dv Halant.dv Nya.dv by JaNya.dv;
	sub Ta.dv Halant.dv Ra.dv by TaRa.dv;
	sub Sha.dv Halant.dv Ra.dv by ShaRa.dv;
	sub vowelIi.dv Bindu.dv by vowelIiBindu.dv;
	sub KaHalant.dv vowelLru.dv by KamatraLru.dv;
	sub KaHalant.dv vowelLruu.dv by KamatraLruu.dv;
	sub Ra.dv Halant.dv ZWJ by ardhaRa.dv;
	sub Ya.dv Halant.dv Chandrabindu.dv by YaHalantCB.dv;
	sub Va.dv Halant.dv Chandrabindu.dv by VaHalantCB.dv;
	sub La.dv Halant.dv Chandrabindu.dv by LaHalantCB.dv;
	sub Visarga.dv V16 by VisargaV16.dv;
	sub Visarga.dv V17 by VisargaV17.dv;
	sub Visarga.dv V18 by VisargaV18.dv;
	sub Visarga.dv V19 by VisargaV19.dv;
	sub Visarga.dv V20 by VisargaV20.dv;
} akhn;

